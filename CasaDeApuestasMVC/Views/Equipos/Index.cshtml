<div class="alert alert-primary">
    <div class="row">
        <div class="col-md-4">
            <h1>Mantenimiento de Equipos</h1>
        </div>
        <div class="col-md-2">
            <button id="btnAgregarEquipos" class="btn btn-success">
                Agregar  
            </button>
        </div>
    </div>
</div>

<div id="divListadoEquipos" class="divListadoEquipos">

</div>

<script>

    CargarListadoEquipos()
    function CargarListadoEquipos()
    {

        $.ajax({

            type: "GET",
            url: "@Url.Action("Listado","Equipos")",
            cache: false,
            success: function(dataHTMLEquipos) {
                $("#divListadoEquipos").html(dataHTMLEquipos)
            }
        });
    }
</script>
