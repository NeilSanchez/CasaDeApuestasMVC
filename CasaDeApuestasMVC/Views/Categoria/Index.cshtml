<div class="alert alert-primary">
    <div class="row">
        <div class="col-md-4">
            <h1>Mantenimiento de Categorias</h1>
        </div>
        <div class="col-md-2">
            <button id="btnAgregarCategoria" class="btn btn-success">
                Agregar  
            </button>
        </div>
    </div>
</div>

<div id="divListadoCategoria" class="divListadoCategoria">

</div>

<script>

    CargarListadoCategoria()
    function CargarListadoCategoria()
    {

        $.ajax({

            type: "GET",
            url: "@Url.Action("Listado","Categoria")",
            cache: false,
            success: function(dataHTMLCategoria) {
                $("#divListadoCategoria").html(dataHTMLCategoria)
            }
        });
    }
</script>